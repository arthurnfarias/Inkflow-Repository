<p id="notice"><%= notice %></p>

<h1>Okrs</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Team</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @okrs.each do |okr| %>
      <tr>
        <td><%= okr.title %></td>
        <td><%= okr.description %></td>
        <td><%= okr.team_id %></td>
        <td><%= link_to 'Show', {:action => 'show', :id => okr.id} %></td>
        <td><%= link_to 'Edit', {:action => 'edit', :id => okr.id} %></td>
        <td><%= link_to 'Destroy', {:action => 'destroy', :id => okr.id}, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Okr', new_team_okr_path %>
